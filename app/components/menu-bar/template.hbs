<PortalFor @name="menu-bar-extras" />
<a href="#" {{action "switchTheme"}} title="Switch theme"><SvgIcon @name="droplet" /></a>
{{#if session.me}}
  <LinkTo @route="feed" title="Go to feed"><SvgIcon @name="inbox" /></LinkTo>
  <a href="/users/sign_out" title="Sign out {{session.me.email}}"><SvgIcon @name="log-out" /></a>
{{else}}
  <a href="/users/sign_in" title="Sign in"><SvgIcon @name="log-in" /></a>
{{/if}}
